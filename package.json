{
  "name": "@hifi/liquidator-js",
  "description": "Utility for automatically liquidating underwater accounts in Hifi",
  "version": "1.0.0",
  "author": {
    "name": "Hifi",
    "email": "contact@hifi.finance",
    "url": "https://hifi.finance"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.0",
    "typescript": "~4.3.5"
  },
  "files": [
    "/dist",
    "/types"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "node .",
    "dev": "ts-node .",
    "build": "tsc --build tsconfig.prod.json",
    "clean": "shx rm -rf ./dist",
    "commit": "git-cz",
    "lint": "yarn run lint:ts && yarn run prettier:check",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --config ./.prettierrc --write \"**/*.{js,json,md,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc \"**/*.{js,json,md,ts}\""
  },
  "types": "./types/index.d.ts",
  "dependencies": {
    "@hifi/flash-swap": "^1.3.1",
    "@hifi/protocol": "^1.4.0",
    "ethers": "^5.4.6",
    "stormdb": "^0.5.2"
  }
}
