{
  "name": "@hifi/liquidator-js",
  "description": "Utility for automatically liquidating underwater accounts in Hifi",
  "version": "1.0.4",
  "author": {
    "name": "Hifi",
    "email": "contact@hifi.finance",
    "url": "https://hifi.finance"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "pinst": "^2.1.6",
    "prettier": "^2.6.2",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "typescript": "~4.6.3"
  },
  "files": [
    "/types"
  ],
  "license": "MIT",
  "main": "./dist",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --build tsconfig.prod.json",
    "clean": "shx rm -rf ./dist",
    "commit": "git-cz",
    "dev": "ts-node src",
    "lint": "yarn run lint:ts && yarn run prettier:check",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --config ./.prettierrc --write \"**/*.{js,json,md,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc \"**/*.{js,json,md,ts}\"",
    "start": "node ."
  },
  "types": "./types/index.d.ts",
  "dependencies": {
    "@hifi/flash-swap": "^1.10.0",
    "@hifi/protocol": "^1.10.0",
    "dotenv": "^10.0.0",
    "ethers": "^5.6.2",
    "stormdb": "^0.5.2",
    "utils": "^0.3.1",
    "winston": "^3.7.1"
  }
}
